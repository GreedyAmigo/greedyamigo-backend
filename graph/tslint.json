{
  "extends": "tslint-config-airbnb",
  "max-line-length": [
    true,
    {
      "limit": 140,
      "ignore-pattern": "^import |^export {(.*?)}"
    }
  ],
  "linterOptions": {
    "exclude": [
      "**/generated/tmp-resolvers/*.ts",
      "**/generated/prisma-client/*.d.ts",
      "**/generated/*.ts"
    ]
  }
}